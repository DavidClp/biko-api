{
  "name": "listup-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/shared/infra/http/express/server.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/shared/infra/http/express/server.ts",
    "dev:watch": "nodemon --exec ts-node src/index.ts",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull && prisma generate",
    "db:generate": "prisma generate",
    "build": "yarn remove sharp && yarn add sharp@0.33.3 --ignore-engines && rm -rf build && yarn db:generate && tsc",
    "build-windows": "yarn remove sharp && yarn add sharp@0.33.3 --ignore-engines && rimraf build && yarn db:generate && tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.15.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "socket.io": "^4.8.1"
  }
}
